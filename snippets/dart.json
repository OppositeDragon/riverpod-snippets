      "\tconst $1({super.key});\n",
      "\t@override",
      "\tConsumerState<$1> createState() => _$1State();",
      "}\n",
      "class _$1State extends ConsumerState<$1> {\n",
      "\t@override",
      "\tWidget build(BuildContext context) {",
      "\t\treturn ${2:Placeholder()};",
      "\t}",
      "}"
    ]
  },
