{
  //Riverpod with code generation
  "Riverpod part statement": {
    "prefix": "riverpodPart",
    "description": "Creates a part statement for Riverpod. (needed for code generation)",
    "body": [
      "part '${1:file_name}.g.dart';",
      "$0"
    ]
  },
  "Riverpod Provider": {
    "prefix": "riverpod",
    "description": "Creates a basic Riverpod Provider that uses code generation",
    "body": [
      "@riverpod",
      "$1 $2(Ref ref) {$3",
      "\treturn $4;",
      "}",
      "$0"
    ]
  },
  "Riverpod Keep Alive Provider": {
    "prefix": "riverpodKeepAlive",
    "description": "Creates a non-autodispose Riverpod Provider that uses code generation",
    "body": [
      "@Riverpod(keepAlive: true)",
      "$1 $2(Ref ref) {$3",
      "\treturn $4;",
      "}",
      "$0"
    ]
  },
  "Riverpod FutureProvider": {
